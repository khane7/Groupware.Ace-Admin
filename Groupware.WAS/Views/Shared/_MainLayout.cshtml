@{
	var bootstrap_themes = "ace-v1.2--bs-v3.0.0";
	
	if ( Session["emp_no"] == null || Session["emp_no"] == "" )
	{
		Html.RenderAction("Index", "Login");
	}
}
<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <title>@ViewBag.Title</title>
	<!--jquery-->
	@Styles.Render("~/Content/css")
	@Scripts.Render("~/bundles/modernizr")
	@Scripts.Render("~/bundles/jquery")


	<!-- jquery library -->
	@Scripts.Render("~/jquerylib/jstree-js")
	@Styles.Render("~/jquerylib/jstree-css")



	<!-- START : Bootstrap Templete -->
		<!-- basic styles -->

		<link href="~/Bootstrap/@bootstrap_themes/assets/css/bootstrap.min.css" rel="stylesheet" />
		<link rel="stylesheet" href="~/Bootstrap/@bootstrap_themes/assets/css/font-awesome.min.css" />

		<!--[if IE 7]>
		  <link rel="stylesheet" href="~/Bootstrap/@bootstrap_themes/assets/css/font-awesome-ie7.min.css" />
		<![endif]-->

		<!-- page specific plugin styles -->
		
		<link rel="stylesheet" href="~/Bootstrap/@bootstrap_themes/assets/css/jquery-ui-1.10.3.full.min.css" />
		<script src="~/Bootstrap/@bootstrap_themes/assets/js/jquery-ui-1.10.3.full.min.js"></script>
		<script src="~/Bootstrap/@bootstrap_themes/assets/js/jquery.ui.touch-punch.min.js"></script>

		<!-- fonts -->

		<link rel="stylesheet" href="~/Bootstrap/@bootstrap_themes/assets/css/ace-fonts.css" />

		<!-- ace styles -->

		<link rel="stylesheet" href="~/Bootstrap/@bootstrap_themes/assets/css/ace.min.css" />
		<link rel="stylesheet" href="~/Bootstrap/@bootstrap_themes/assets/css/ace-rtl.min.css" />
		<link rel="stylesheet" href="~/Bootstrap/@bootstrap_themes/assets/css/ace-skins.min.css" />

		<!--[if lte IE 8]>
		  <link rel="stylesheet" href="~/Bootstrap/@bootstrap_themes/assets/css/ace-ie.min.css" />
		<![endif]-->

		<!-- inline styles related to this page -->

		<!-- ace settings handler -->

		<script src="~/Bootstrap/@bootstrap_themes/assets/js/ace-extra.min.js"></script>

		<!-- HTML5 shim and Respond.js IE8 support of HTML5 elements and media queries -->

		<!--[if lt IE 9]>
		<script src="~/Bootstrap/@bootstrap_themes/assets/js/html5shiv.js"></script>
		<script src="~/Bootstrap/@bootstrap_themes/assets/js/respond.min.js"></script>
		<![endif]-->
	<!-- END : Bootstrap Templete -->

	<!-- Groupware JS -->
	@Scripts.Render("~/custom/common-js")


</head>
<body>

	<div class="navbar navbar-default" id="navbar">
		@{
			Html.RenderAction("Index", "Header"); 
		}
	</div>

	<div class="main-container" id="main-container">
		<script type="text/javascript">
			try { ace.settings.check('main-container', 'fixed') } catch (e) { }
		</script>

		<div class="main-container-inner">
			<a class="menu-toggler" id="menu-toggler" href="#">
				<span class="menu-text"></span>
			</a>

			<div class="sidebar" id="sidebar">
				@{
					Html.RenderAction("Index", "Left");
				}
			</div>

			<div class="main-content">
				@RenderBody()
			</div>

			<!-- 환경설정 -->
			<div class="ace-settings-container" id="ace-settings-container">
				<div class="btn btn-app btn-xs btn-warning ace-settings-btn" id="ace-settings-btn">
					<i class="icon-cog bigger-150"></i>
				</div>

				<div class="ace-settings-box" id="ace-settings-box">
					<div>
						<div class="pull-left">
							<select id="skin-colorpicker" class="hide">
								<option data-skin="default" value="#438EB9">#438EB9</option>
								<option data-skin="skin-1" value="#222A2D">#222A2D</option>
								<option data-skin="skin-2" value="#C6487E">#C6487E</option>
								<option data-skin="skin-3" value="#D0D0D0">#D0D0D0</option>
							</select>
						</div>
						<span>&nbsp; Choose Skin</span>
					</div>

					<div>
						<input type="checkbox" class="ace ace-checkbox-2" id="ace-settings-navbar" />
						<label class="lbl" for="ace-settings-navbar"> Fixed Navbar</label>
					</div>

					<div>
						<input type="checkbox" class="ace ace-checkbox-2" id="ace-settings-sidebar" />
						<label class="lbl" for="ace-settings-sidebar"> Fixed Sidebar</label>
					</div>

					<div>
						<input type="checkbox" class="ace ace-checkbox-2" id="ace-settings-breadcrumbs" />
						<label class="lbl" for="ace-settings-breadcrumbs"> Fixed Breadcrumbs</label>
					</div>

					<div>
						<input type="checkbox" class="ace ace-checkbox-2" id="ace-settings-rtl" />
						<label class="lbl" for="ace-settings-rtl"> Right To Left (rtl)</label>
					</div>

					<div>
						<input type="checkbox" class="ace ace-checkbox-2" id="ace-settings-add-container" />
						<label class="lbl" for="ace-settings-add-container">
							Inside
							<b>.container</b>
						</label>
					</div>
				</div>
			</div><!-- /#ace-settings-container -->

		</div>

	</div>

	
	@RenderSection("scripts", required: false)


	<!-- START : Bootstrap Templete -->
		<!-- basic scripts -->

		<!--[if !IE]> -->

		<script type="text/javascript">
			window.jQuery || document.write("<script src='~/Bootstrap/@bootstrap_themes/assets/js/jquery-2.0.3.min.js'>" + "<" + "/script>");
		</script>

		<!-- <![endif]-->

		<!--[if IE]>
		<script type="text/javascript">
		 window.jQuery || document.write("<script src='~/Bootstrap/@bootstrap_themes/assets/js/jquery-1.10.2.min.js'>"+"<"+"/script>");
		</script>
		<![endif]-->

		<script type="text/javascript">
			if ("ontouchend" in document) document.write("<script src='~/Bootstrap/@bootstrap_themes/assets/js/jquery.mobile.custom.min.js'>" + "<" + "/script>");
		</script>
		<script src="~/Bootstrap/@bootstrap_themes/assets/js/bootstrap.min.js"></script>
		<script src="~/Bootstrap/@bootstrap_themes/assets/js/typeahead-bs2.min.js"></script>

		<!-- page specific plugin scripts -->

		<!-- ace scripts -->

		<script src="~/Bootstrap/@bootstrap_themes/assets/js/ace-elements.min.js"></script>
		<script src="~/Bootstrap/@bootstrap_themes/assets/js/ace.min.js"></script>

		<!-- inline scripts related to this page -->
	<!-- END : Bootstrap Templete -->

</body>

@RenderPage("~/Views/Common/Dialog/DialogModal.cshtml")

</html>
